---
import SEO from "../components/SEO.astro"
const {
  title,
  description,
  image,
  url = Astro.url,
  type = 'website',
} = Astro.props;
---


<meta charset="utf-8" />
<link rel="icon" type="image/svg+xml" href={image} />
<meta name="viewport" content="width=device-width" />
<meta name="generator" content={Astro.generator} />
<SEO {title} {description} {image} {url} {url}/>
<title>{title}</title>
<link "is:inline" href="//vjs.zencdn.net/8.21.1/video-js.min.css" rel="stylesheet">
<script  "is:inline" src="//vjs.zencdn.net/8.21.1/video.min.js"></script>
