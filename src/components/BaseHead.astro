---
import SEO from "../components/SEO.astro"
const {
  title,
  description,
  image,
  url = Astro.url,
  type = 'website',
} = Astro.props;
---


<meta charset="utf-8" />
<link rel="icon" type="image/svg+xml" href={image} />
<meta name="viewport" content="width=device-width" />
<meta name="generator" content={Astro.generator} />
<SEO {title} {description} {image} {url} {url}/>
<title>{title}</title>
<link "is:inline" href="//vjs.zencdn.net/8.21.1/video-js.min.css" rel="stylesheet">
<script  "is:inline" src="//vjs.zencdn.net/8.21.1/video.min.js"></script>
<!-- Matomo -->
<script>
  var _paq = window._paq = window._paq || [];
  /* tracker methods like "setCustomDimension" should be called before "trackPageView" */
  _paq.push(["setExcludedReferrers", ["127.0.0.1:4000\/"]]);
  _paq.push(['trackPageView']);
  _paq.push(['enableLinkTracking']);
  (function() {
    var u="https://matomo.ganglha.net/";
    _paq.push(['setTrackerUrl', u+'matomo.php']);
    _paq.push(['setSiteId', '4']);
    var d=document, g=d.createElement('script'), s=d.getElementsByTagName('script')[0];
    g.async=true; g.src=u+'matomo.js'; s.parentNode.insertBefore(g,s);
  })();
</script>
<!-- End Matomo Code -->
